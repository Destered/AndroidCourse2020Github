<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView_addIcon4"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="20dp"
        android:maxWidth="24dp"
        android:maxHeight="24dp"
        android:minWidth="24dp"
        android:minHeight="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView_addIcon3"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageView_addIcon2"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="20dp"
        android:maxWidth="24dp"
        android:maxHeight="24dp"
        android:minWidth="24dp"
        android:minHeight="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView_addIcon1"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageView_addIcon3"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="20dp"
        android:maxWidth="24dp"
        android:maxHeight="24dp"
        android:minWidth="24dp"
        android:minHeight="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView_addIcon2"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageView_avatar"
        android:layout_width="83dp"
        android:layout_height="81dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="48dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:layout_marginTop="8dp"
        android:text="atari_dnd"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="48dp"
        android:text="Тимур Болтачёв"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/imageView_avatar"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="6dp"
        android:text="Расскажите о себе"
        android:textColor="#01579B"
        app:layout_constraintStart_toEndOf="@+id/imageView_avatar"
        app:layout_constraintTop_toBottomOf="@+id/tv_name" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="8dp"
        android:text="был в сети 4 минуты назад"
        android:textColor="#666666"
        app:layout_constraintStart_toEndOf="@+id/imageView_avatar"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
        android:id="@+id/btn_edit"
        android:layout_width="366dp"
        android:layout_height="39dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:backgroundTint="#E6E6E6"
        android:text="Редактировать"
        android:textColor="#01579B"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <EditText
        android:id="@+id/et_name"
        android:layout_width="153dp"
        android:layout_height="26dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="48dp"
        android:background="@null"
        android:ems="20"
        android:inputType="text"
        app:layout_constraintStart_toEndOf="@+id/imageView_avatar"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView_icon1"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="19dp"
        android:maxWidth="64dp"
        android:maxHeight="64dp"
        android:minWidth="64dp"
        android:minHeight="64dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_edit"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageView_icon2"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginStart="24dp"
        android:maxWidth="64dp"
        android:maxHeight="64dp"
        android:minWidth="64dp"
        android:minHeight="64dp"
        app:layout_constraintStart_toEndOf="@+id/imageView_icon1"
        app:layout_constraintTop_toTopOf="@+id/imageView_icon1"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageView_icon3"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginStart="24dp"
        android:maxWidth="64dp"
        android:maxHeight="64dp"
        android:minWidth="64dp"
        android:minHeight="64dp"
        app:layout_constraintStart_toEndOf="@+id/imageView_icon2"
        app:layout_constraintTop_toTopOf="@+id/imageView_icon2"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageView_icon4"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginStart="24dp"
        android:maxWidth="64dp"
        android:maxHeight="64dp"
        android:minWidth="64dp"
        android:minHeight="64dp"
        app:layout_constraintStart_toEndOf="@+id/imageView_icon3"
        app:layout_constraintTop_toTopOf="@+id/imageView_icon3"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:text="Укажите место работы"
        android:textColor="#01579B"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/imageView_addIcon3"
        app:layout_constraintTop_toTopOf="@+id/imageView_addIcon3" />

    <TextView
        android:id="@+id/tv_school"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:text="Место учёбы: Школа № 10"
        android:textColor="#666666"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/imageView_addIcon1"
        app:layout_constraintTop_toTopOf="@+id/imageView_addIcon1" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:text="27 подписчиков"
        android:textColor="#666666"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/imageView_addIcon2"
        app:layout_constraintTop_toTopOf="@+id/imageView_addIcon2" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:text="Подробная информация"
        android:textColor="#01579B"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/imageView_addIcon4"
        app:layout_constraintTop_toTopOf="@+id/imageView_addIcon4" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="32dp"
        android:text="ФОТОГРАФИИ"
        android:textColor="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="4"
        android:textColor="#666666"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toTopOf="@+id/textView9" />

    <ImageView
        android:id="@+id/imageView_photo1"
        android:layout_width="105dp"
        android:layout_height="86dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="8dp"
        android:maxWidth="105dp"
        android:maxHeight="85dp"
        android:minWidth="105dp"
        android:minHeight="85dp"
        android:scaleType="fitXY"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageView_photo2"
        android:layout_width="105dp"
        android:layout_height="86dp"
        android:layout_marginStart="8dp"
        android:maxWidth="105dp"
        android:maxHeight="85dp"
        android:minWidth="105dp"
        android:minHeight="85dp"
        android:scaleType="fitXY"
        app:layout_constraintStart_toEndOf="@+id/imageView_photo1"
        app:layout_constraintTop_toTopOf="@+id/imageView_photo1"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageView_photo3"
        android:layout_width="105dp"
        android:layout_height="86dp"
        android:layout_marginStart="8dp"
        android:maxWidth="105dp"
        android:maxHeight="85dp"
        android:minWidth="105dp"
        android:minHeight="85dp"
        android:scaleType="fitXY"
        app:layout_constraintStart_toEndOf="@+id/imageView_photo2"
        app:layout_constraintTop_toTopOf="@+id/imageView_photo2"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageView_photo4"
        android:layout_width="105dp"
        android:layout_height="86dp"
        android:layout_marginTop="8dp"
        android:maxWidth="105dp"
        android:maxHeight="85dp"
        android:minWidth="105dp"
        android:minHeight="85dp"
        android:scaleType="fitXY"
        app:layout_constraintStart_toStartOf="@+id/imageView_photo1"
        app:layout_constraintTop_toBottomOf="@+id/imageView_photo1"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageView_addIcon1"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="20dp"
        android:maxWidth="24dp"
        android:maxHeight="24dp"
        android:minWidth="24dp"
        android:minHeight="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView_icon1"
        tools:srcCompat="@tools:sample/avatars" />
</androidx.constraintlayout.widget.ConstraintLayout>